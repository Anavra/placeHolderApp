<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.placeholder_inventory">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_box"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_box"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name="com.example.android.placeholder_inventory.AuthActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.android.placeholder_inventory.RoomListActivity"
            android:label="@string/my_rooms">
            </activity>
    </application>

</manifest>