<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.placeholder_inventory">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_box"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_box"
        android:supportsRtl="true"
        android:theme="@style/PlaceHolderTheme">
        <activity
            android:name=".Activities.AuthActivity"
            android:theme="@style/PlaceHolderTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:theme="@style/PlaceHolderTheme"
            android:name=".Activities.RoomListActivity"
            android:label="@string/my_rooms" />
    </application>

</manifest>