<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.android_app">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_box"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_box"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name=".roomListActivity"
            android:label="@string/rooms_activity"
            />
        <activity android:name=".LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".SettingsActivity"
            android:label = "@string/settings_activity"
            android:parentActivityName=".roomListActivity"
            android:exported="true">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.android.android_app.roomListActivity" />
        </activity>

    </application>

</manifest>